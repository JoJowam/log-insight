{
  "name": "log-insight",
  "displayName": "Log Insight",
  "description": "Boost your JavaScript/TypeScript debugging. Log Insight lets you instantly insert highly-customizable console.log statements with context like function name, file, and line number. Stop writing logs by hand and start debugging faster.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "log-insight.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "log-insight.logVariable",
        "title": "Insert console.log for variable"
      },
      {
        "command": "log-insight.debugSelection",
        "title": "Insert debug logs for selection"
      }
    ],
    "keybindings": [
      {
        "command": "log-insight.logVariable",
        "key": "ctrl+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "log-insight.debugSelection",
        "key": "ctrl+shift+d",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "log-insight.logVariable",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "log-insight.debugSelection",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "commit": "cz",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "compile-tests": "tsc -p . --outDir out",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "prepare": "husky",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "release": "standard-version",
    "test": "vscode-test",
    "vscode:prepublish": "pnpm run package",
    "watch": "npm-run-all -p watch:*",
    "watch-tests": "tsc -p . -w --outDir out",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.3"
  }
}
